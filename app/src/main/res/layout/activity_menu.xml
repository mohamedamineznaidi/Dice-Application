<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.dice.znaidi.dice.Menu">

    <ImageButton
        android:id="@+id/dicebt"
        android:layout_width="280dp"
        android:layout_height="78dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:srcCompat="@drawable/dicemenu"
        app:layout_constraintBottom_toTopOf="@+id/sfbt"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.439"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="0.506" />

    <ImageButton
        android:id="@+id/sfbt"
        android:layout_width="279dp"
        android:layout_height="76dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:srcCompat="@drawable/sfmenubt"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/dicebt" />

</android.support.constraint.ConstraintLayout>
